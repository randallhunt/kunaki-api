{
	"name": "kunaki-api",
	"version": "1.0.0",
	"description": "client library for Kunaki's XML API",
	"main": "index.js",
	"exports": {
		".": {
			"import": "./lib/index.js",
			"require": "./lib/index.cjs"
		}
	},
	"types": "./lib/index.d.ts",
	"files": [
		"lib/",
		"types/",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"dev": "npm run build -- --watch",
		"knip": "knip",
		"lint:code": "eslint ./src/**",
		"lint:tests": "eslint ./tests/**",
		"lint": "npm-run-all --parallel lint:*",
		"prepack": "npm run build",
		"publish:npm": "np --no-yarn",
		"start": "npm run dev",
		"test": "uvu --require tsm tests"
	},
	"keywords": [
		"kunaki",
		"api",
		"xml"
	],
	"author": "R. Hunt",
	"engines": {
		"node": ">=18 || >=20"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:randallhunt/kunaki-api.git"
	},
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "^9.10.0",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^22.5.5",
		"eslint": "^9.10.0",
		"knip": "^5.30.2",
		"msw": "^2.4.6",
		"np": "^10.0.7",
		"npm-run-all": "^4.1.5",
		"tsm": "^2.3.0",
		"tsup": "^8.2.4",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.5.0",
		"uvu": "^0.5.6"
	},
	"dependencies": {
		"fast-xml-parser": "^4.5.0"
	}
}
