{
  "name": "kunaki-api",
  "version": "1.0.0",
  "description": "client library for Kunaki's XML API",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "types": "./lib/index.d.ts",
  "files": [
    "lib/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "npm run build -- --watch",
    "fix": "eslint --fix ./src",
    "knip": "knip",
    "lint:code": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "prepack": "npm run build",
    "prepare": "husky",
    "publish:npm": "np --no-yarn",
    "start": "npm run dev",
    "test": "uvu --require tsm tests"
  },
  "keywords": [
    "kunaki",
    "api",
    "xml"
  ],
  "author": "R. Hunt",
  "engines": {
    "node": ">=18 || >=20"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.5.5",
    "eslint": "^9.10.0",
    "knip": "^5.30.2",
    "msw": "^2.4.6",
    "np": "^10.0.7",
    "prettier": "^3.3.3",
    "tsm": "^2.3.0",
    "typescript": "^5.6.2",
    "uvu": "^0.5.6"
  },
  "dependencies": {
    "fast-xml-parser": "^4.5.0"
  }
}
